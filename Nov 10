let object = {
  1: { name: 'Dani', score: 100, age: 32 },
  2: { name: 'Jordi', score: 100, age: 3 }, 
  3: { name: 'Jordi', score: 100, age: 33 }
}

const sortedByScore = Object.values(object).sort((a, b) => {
  if (a.score < b.score) {
    return -1;
  }
  if (a.score > b.score) {
    return 1;
  }
 if (a.age < b.age) {
    return -1;
  }
  if (a.age > b.age) {
    return 1;
  }
  return 0
});

console.log(object)





console.log(sortedByScore)